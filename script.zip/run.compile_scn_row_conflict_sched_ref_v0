#!/bin/bash

# Usage: ./run.compile_scn_row_conflict_sched_ref_v0
STEPN=2
source ./common.sh $STEPN

SCN_NAME="ROW_CONFLICT"
SCHED_TAG="REF_V0"
FILELIST="${ROOT_PATH}/script/filelist_step2_scn_row_conflict_sched_ref_v0.f"
RUN_DIR="${ROOT_PATH}/script/OUTPUT_STEP2_${SCN_NAME}_${SCHED_TAG}"

echo "Cleaning up the old directory"
rm -rf $RUN_DIR
echo "Creating a new directory"
mkdir -p $RUN_DIR
cd $RUN_DIR
echo "Compiling"
$COMPILE_CMD $COMPILE_OPTIONS $COMPILE_INCDIR -f $FILELIST
